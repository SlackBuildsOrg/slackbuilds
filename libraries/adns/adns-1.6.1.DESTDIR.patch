 AC_INSTALL=	@INSTALL@
 ifeq ($(AC_INSTALL),./install-sh -c)
 INSTALL=	$(srcdir)/../$(AC_INSTALL)
@@ -67,4 +56,4 @@ endif
 
 INSTALL_PROGRAM=	$(INSTALL) -m 755 $(INSTALL_PROGRAM_FLAGS)
 INSTALL_DATA=		$(INSTALL) -m 644
-INSTALL_DIR=		$(INSTALL) -d 755
+INSTALL_DIR=		$(INSTALL) -m 755 -d
